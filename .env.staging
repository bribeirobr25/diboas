# Staging Environment Configuration
# Contains staging-specific configurations
# Real credentials should be set via CI/CD or deployment system

# Application Environment
VITE_APP_ENV=staging
VITE_APP_REGION=global
VITE_APP_VERSION=1.0.0-staging
VITE_BUILD_TIME=

# Staging API Credentials (Set via deployment system)
VITE_STAGING_API_KEY=
VITE_STAGING_CLIENT_ID=
VITE_STAGING_ENCRYPTION_KEY=

# Staging Services
VITE_STAGING_API_BASE_URL=https://api-staging.diboas.com/api
VITE_STAGING_AUTH_URL=https://auth-staging.diboas.com
VITE_STAGING_WS_URL=wss://ws-staging.diboas.com
VITE_STAGING_CDN_URL=https://cdn-staging.diboas.com

# Staging Feature Flags (Mirror production with some experimental features)
VITE_FF_ENHANCED_AUTHENTICATION=true
VITE_FF_CRYPTO_WALLET_INTEGRATION=true
VITE_FF_NEW_DASHBOARD_DESIGN=true
VITE_FF_AI_FINANCIAL_ADVISOR=true
VITE_FF_DARK_MODE=true
VITE_FF_DEFI_INVESTMENTS=true

# Staging Debug Settings
VITE_DEBUG_MODE=true
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEV_TOOLS=true
VITE_LOG_LEVEL=info
VITE_CACHE_ENABLED=true

# Staging Analytics (Enabled for testing)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_TRACKING=true

# External Services (Real staging endpoints)
VITE_MOCK_PAYMENT_PROVIDERS=false
VITE_MOCK_BLOCKCHAIN_APIS=false
VITE_MOCK_MARKET_DATA=false

# Staging Database
VITE_DB_HOST=staging-db.diboas.com
VITE_DB_PORT=5432
VITE_DB_NAME=diboas_staging

# Staging Security
VITE_SSL_REQUIRED=true
VITE_API_TIMEOUT=25000
VITE_RETRY_ATTEMPTS=2