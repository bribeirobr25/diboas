# Production Environment Configuration
# NEVER commit real production secrets to version control
# All sensitive values should be set via secure deployment systems

# Application Environment
VITE_APP_ENV=production
VITE_APP_REGION=
VITE_APP_VERSION=1.0.0
VITE_BUILD_TIME=

# Production API Credentials (Set via secure deployment system)
# NEVER SET REAL VALUES HERE - USE CI/CD SECRETS MANAGEMENT
VITE_PROD_API_KEY=
VITE_PROD_CLIENT_ID=
VITE_PROD_ENCRYPTION_KEY=

# Production Services
VITE_PROD_API_BASE_URL=https://api.diboas.com/api
VITE_PROD_AUTH_URL=https://auth.diboas.com
VITE_PROD_WS_URL=wss://ws.diboas.com
VITE_PROD_CDN_URL=https://cdn.diboas.com

# Production Feature Flags (Conservative approach)
VITE_FF_ENHANCED_AUTHENTICATION=true
VITE_FF_CRYPTO_WALLET_INTEGRATION=false  # Gradual rollout via feature flag system
VITE_FF_NEW_DASHBOARD_DESIGN=false       # A/B testing via feature flag system
VITE_FF_AI_FINANCIAL_ADVISOR=false       # Beta users only via feature flag system
VITE_FF_DARK_MODE=true
VITE_FF_DEFI_INVESTMENTS=false           # Premium users only via feature flag system

# Production Debug Settings (Minimal for security)
VITE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEV_TOOLS=false
VITE_LOG_LEVEL=error
VITE_CACHE_ENABLED=true

# Production Analytics (Enabled)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_TRACKING=true

# External Services (Real production endpoints)
VITE_MOCK_PAYMENT_PROVIDERS=false
VITE_MOCK_BLOCKCHAIN_APIS=false
VITE_MOCK_MARKET_DATA=false

# Production Database
VITE_DB_HOST=
VITE_DB_PORT=5432
VITE_DB_NAME=diboas_production

# Production Security
VITE_SSL_REQUIRED=true
VITE_API_TIMEOUT=20000
VITE_RETRY_ATTEMPTS=1

# CDN and Asset Optimization
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_MINIFICATION=true
VITE_ENABLE_TREE_SHAKING=true

# Monitoring and Observability
VITE_ENABLE_APM=true
VITE_ENABLE_HEALTH_CHECKS=true