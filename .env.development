# Development Environment Configuration
# Safe to commit - contains no production secrets

# Application Environment
VITE_APP_ENV=development
VITE_APP_REGION=global
VITE_APP_VERSION=1.0.0-dev
VITE_BUILD_TIME=

# Development API Credentials (Safe placeholder values)
VITE_DEV_API_KEY=dev-api-key-placeholder-safe-to-commit
VITE_DEV_CLIENT_ID=dev-client-id-placeholder
VITE_DEV_ENCRYPTION_KEY=dev-encryption-placeholder

# Development Services
VITE_DEV_API_BASE_URL=http://localhost:3001/api
VITE_DEV_AUTH_URL=http://localhost:3001/auth
VITE_DEV_WS_URL=ws://localhost:3001
VITE_DEV_CDN_URL=http://localhost:3001/assets

# Development Feature Flags Override
VITE_FF_ENHANCED_AUTHENTICATION=true
VITE_FF_CRYPTO_WALLET_INTEGRATION=true
VITE_FF_NEW_DASHBOARD_DESIGN=true
VITE_FF_AI_FINANCIAL_ADVISOR=true
VITE_FF_DARK_MODE=true

# Development Debug Settings
VITE_DEBUG_MODE=true
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEV_TOOLS=true
VITE_LOG_LEVEL=debug
VITE_CACHE_ENABLED=false

# Development Analytics (Disabled for privacy)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_TRACKING=false

# Mock External Services (for offline development)
VITE_MOCK_PAYMENT_PROVIDERS=true
VITE_MOCK_BLOCKCHAIN_APIS=true
VITE_MOCK_MARKET_DATA=true

# Development Database (if applicable)
VITE_DB_HOST=localhost
VITE_DB_PORT=5432
VITE_DB_NAME=diboas_dev

# Local Development Ports
VITE_DEV_SERVER_PORT=5173
VITE_API_SERVER_PORT=3001
VITE_WS_SERVER_PORT=3002