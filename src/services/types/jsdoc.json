{
  "source": {
    "include": [
      "../**/Mockup*.js"
    ],
    "exclude": [
      "../**/__tests__/**",
      "../**/node_modules/**"
    ]
  },
  "opts": {
    "destination": "../../docs/mockup-services/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}